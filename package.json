{
    "name": "japanese-bracket-highlight",
    "author": {
        "name": "wakahh"
    },
    "icon": "images/icon.png",
    "displayName": "Japanese Bracket Highlight",
    "description": "Highlight Japanese bracket",
    "version": "0.0.4",
    "engines": {
        "vscode": "^1.56.2"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "plaintext",
            "aliases": ["plaintext", "plaintext"],
            "extensions": [".txt"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "plaintext",
            "scopeName": "source.txt",
            "path": "./syntaxes/plaintext.tmLanguage.json"
        }]
    },
    "publisher": "wakahh",
    "repository": {
        "type": "git",
        "url": "https://github.com/wakahh/Japanese-Bracket-Highlight"
    },
    "homepage": "https://github.com/wakahh/Japanese-Bracket-Highlight"
}